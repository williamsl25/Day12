<h1>Patient List</h1>
<%= link_to "Root", root_path %>



<ul>
  <% @patients.each do |patient| %>
  <li>
    <%= patient.first_name %>
    <%= patient.last_name %>
    <%= patient.date_of_birth %>
    <%= patient.description %>
    <br>
    <%= link_to "Edit", edit_patient_path(patient) %>
    <br>
    <%= link_to "Delete", patient_path(patient), method: :delete %>

  </li>
  <% end %>


</ul>